{
  "name": "infra_lib_nodejs",
  "version": "1.6.0",
  "description": "librarie de communication avec la base infra for nodeJS",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "buildGen": "tsc -p ./generator/src/gen-tsconfig.json",
    "generator": "node ./generator/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "testing": "node dist/test.js",
    "pub": "git checkout master && npm version $npm_package_version && git checkout dev",
    "vMajor": "npm version major ",
    "vMinor": "npm version minor && npm run initNewVBrach",
    "initNewVBrach": "git checkout -b V$npm_package_version  &&  git push --set-upstream origin V$npm_package_version && git add -A && git push && npm run pub",
    "vPatch": "npm version patch",
    "version": "git add -A ",
    "postversion": "git push && git push --tags "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HF-dev/infra_lib_nodejs.git"
  },
  "author": "hf-dev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HF-dev/infra_lib_nodejs/issues"
  },
  "homepage": "https://github.com/HF-dev/infra_lib_nodejs#readme",
  "dependencies": {
    "assert": "^1.4.1",
    "fs-extra": "^3.0.1",
    "lodash": "^4.17.4",
    "node-jose": "^1.0.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "utils": "github:hf-dev/utils"
  },
  "devDependencies": {
    "@types/assert": "0.0.31",
    "@types/fs-extra": "^3.0.3",
    "@types/lodash": "^4.14.66",
    "cross-env": "^5.1.1"
  }
}
